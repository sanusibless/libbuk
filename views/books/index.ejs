<h2>All Books in the Library</h2>
<form action="/books" method="GET">
    <input type="text" name="title" value="<%= searchOptions.title %>"placeholder="Search by Title, Author">
    <button type="submit">Search</button>
</form>

<% books.forEach(book => { %> 
    <div><%= book.title %></div>
<% }); %>

<a href="/books/create">Add Book</a>
